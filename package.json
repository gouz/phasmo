{
  "name": "phasmo",
  "version": "1.0.0",
  "repository": "https://github.com/gouz/phasmo.git",
  "author": "Sylvain Gougouzian <sylvain.gougouzian@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "tippy.js": "^6.3.1"
  },
  "browserslist": [
    "> 0.2%",
    "not dead"
  ],
  "targets": {
    "default": {
      "includeNodeModules": true
    }
  },
  "scripts": {
    "preparcel": "rm -rf .parcel-cache dist docs",
    "parcel": "parcel $RUN src/index.pug",
    "build": "RUN=build npm run parcel -- --no-cache --no-source-maps --dist-dir docs",
    "watch": "RUN=watch npm run parcel",
    "dev": "RUN=serve npm run parcel -- --open"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.0.0-beta.3.1",
    "@parcel/transformer-less": "^2.0.0-beta.3.1",
    "@parcel/transformer-pug": "^2.0.0-beta.3.1",
    "@parcel/transformer-webmanifest": "^2.0.0-beta.3.1",
    "autoprefixer": "^10.2.6",
    "parcel": "^2.0.0-beta.3.1",
    "postcss": "^8.3.2",
    "postcss-import": "^14.0.0",
    "pug": "^3.0.2",
    "tailwindcss": "^2.1.4"
  }
}
